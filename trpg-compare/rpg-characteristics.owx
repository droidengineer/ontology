        <IRI>#NumberOfDice</IRI>
        <Literal xml:lang="en">Number of Dice</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#OpenEnded</IRI>
        <Literal>Open Ended</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
